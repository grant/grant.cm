@import "../global"
@import "../reset"

@import "../page"

body
  overflow hidden
  background-color $RED

.calArea
  width 100%
  height 100%

  .title
    width 100%
    padding 15px
    text-align center
    position absolute
    pointer-events none

    font-size 40px
    title-font()
    color white

  .cal
    border-width 0
    padding-top 50px
    width 100%
    height 100%

    iframe
      width 100%
      height 100%